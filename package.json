{
  "private": true,
  "type": "module",
  "scripts": {
    "depchk-debug": "SYNCPACK_VERBOSE=true dotenv -- syncpack lint",
    "depchk": "dotenv -- syncpack lint",
    "dep:sort": "dotenv -- pnpm dlx sherif@latest -f",
    "build": "dotenv -- sh -c 'turbo run build --filter \"$BUNDLER\"'",
    "build:all": "dotenv -- turbo run build",
    "build-storybook": "dotenv -- turbo run build-storybook",
    "build-storybook-docs": "dotenv -- turbo run build-storybook-docs",
    "dev": "dotenv -- sh -c 'turbo run dev --filter \"$BUNDLER\"'",
    "dev:all": "dotenv -- turbo run dev",
    "lint": "dotenv -- turbo run lint",
    "format": "dotenv -- prettier . --write",
    "check:format": "dotenv -- prettier . --check",
    "test": "dotenv -- turbo run test",
    "test:watch": "dotenv -- turbo run test:watch",
    "test:type": "dotenv -- turbo run test:type",
    "clear:cache": "pnpm cache delete && rm -rf pnpm-lock.yaml && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "check-all": "dotenv -- pnpm depchk && pnpm test:type && pnpm lint && pnpm test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "lint-staged": "16.0.0",
    "prettier": "^3.5.3",
    "syncpack": "13.0.4",
    "turbo": "2.5.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.8.0",
  "name": "turborepo-react-starter"
}
