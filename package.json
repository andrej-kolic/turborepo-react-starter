{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:type": "turbo run test:type",
    "clear:cache": "rm -rf pnpm-lock.yaml && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "prettier": "^3.3.3",
    "tsconfig": "workspace:*",
    "turbo": "2.1.0",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "turborepo-react-starter"
}
