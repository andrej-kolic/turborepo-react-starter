{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Webpack.js",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable"],
    "module": "esnext",
    "resolveJsonModule": true,
    "strict": false,
    "target": "es5"
  },
  "include": ["src", "webpack.d.ts"],
  "exclude": ["node_modules"]
}
